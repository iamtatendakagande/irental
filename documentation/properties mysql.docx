CREATE TABLE irental.properties(
    id int AUTO_INCREMENT PRIMARY KEY NOT NULL,
	email varchar(35) NOT NULL,
	price DOUBLE(16,2) NOT NULL,
	suburb varchar(35) NOT NULL,
	density varchar(35)NOT NULL,
	type_of_property varchar(35) NOT NULL,
	rooms int NOT NULL,
    bedroom int NOT NULL,
	toilets int NOT NULL,
	ensuite int NOT NULL,
	toilets_type varchar(35) NOT NULL,
	garage int NOT NULL,
	swimming_pool int NOT NULL,
    fixtures_fittings int NOT NULL,
	cottage int NOT NULL,
	power int NOT NULL,
	power_backup int NOT NULL,
	water int NOT NULL,
	water_backup int NOT NULL,
	gated_community int NOT NULL,
	garden_area int NOT NULL,
	address varchar(45) NOT NULL,
	description varchar(130)NOT NULL	
);